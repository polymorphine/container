{
    "name": "shudd3r/container",
    "description": "PSR-11 Container for Dependencies & Configuration",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "satooshi/php-coveralls": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shudd3r",
            "email": "q3.shudder@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shudd3r\\Http\\Src\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shudd3r\\Http\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "run-tests": "phpunit --configuration phpunit.xml.dist"
    }
}
