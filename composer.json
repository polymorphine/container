    "prefer-stable": true,
    "require": {
        "php": "^7.4",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "polymorphine/dev": "0.1.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shudd3r",
            "email": "q3.shudder@gmail.com"
        }
    ],
    "autoload": {
