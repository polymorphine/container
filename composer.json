{
    "name": "polymorphine/container",
    "description": "PSR-11 Container for libraries & configuration",
    "type": "library",
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "polymorphine/cs": "1.1",
        "phpunit/phpunit": "^8.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shudd3r",
            "email": "q3.shudder@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Polymorphine\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Polymorphine\\Container\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "run-tests": "phpunit --configuration phpunit.xml.dist",
        "style-check": "polymorphine-cs"
    }
}
